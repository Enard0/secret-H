{
    // For more information, visit: https://go.microsoft.com/fwlink/?LinkId=733558
    "version": "2.0.0",
    "tasks": [
      {
        "type": "npm",
        "script": "dev",
        "isBackground": true,
        "problemMatcher": {
          "owner": "JavaScript",
          "pattern": "$js",
          "background": {
            "activeOnStart": true,
            "beginsPattern": {
              "regexp": "(.*?)"
            },
            "endsPattern": {
              "regexp": ".*ready.*"
            }
          }
        },
        "options": {
          "cwd": "${workspaceFolder}/front/src/"
        }
        }
    ]
  }